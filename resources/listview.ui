<interface>
  <!-- interface-name builder.ui -->
  <requires lib="gtk" version="4.0"/>
  <object class="GtkApplicationWindow" id="window">
    <property name="default-height">800</property>
    <property name="default-width">600</property>
    <property name="show-menubar">True</property>
    <property name="title">Commander</property>
    <child type="titlebar">
      <object class="GtkHeaderBar" id="header">
        <child type="title">
          <!-- <object class="GtkStackSwitcher" id="tabs">
            <property name="stack">stack</property>
          </object> -->
        </child>
        <child type="end">
          <object class="GtkMenuButton" id="gears">
            <property name="direction">none</property>
          </object>
        </child>
        <child type="end">
          <object class="GtkToggleButton" id="search">
            <property name="sensitive">0</property>
            <property name="icon-name">edit-find-symbolic</property>
          </object>
        </child>
      </object>
    </child>        
    <child>
      <object class="GtkPaned">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkPaned">
            <child>
              <object class="GtkBox">
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkButton">
                    <property name="label">Left Folder</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox">
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkButton">
                    <property name="label">Right Folder</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="viewer">
            <property name="label">Viewer</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
